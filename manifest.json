{
  "manifest_version": 3,
  "name": "HubSpot Layout Manager",
  "version": "1.0",
  "description": "Editor enhancement for HubSpot design manager",
  "permissions": [
    "activeTab"
  ],
  "host_permissions": [
    "https://app.hubspot.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://app.hubspot.com/design-manager/*"
      ],
      "js": [
        "pages/design-manager.js",
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}